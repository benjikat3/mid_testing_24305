<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <welcome-file-list>
        <welcome-file>
            login.jsp
        </welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>SignupServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.SignupServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocationServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.LocationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.BookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrowServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.BorrowServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MyBorrowedBookServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.MyBorrowedBookServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewMemberServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.ViewMemberServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewLibraryServlet</servlet-name>
        <servlet-class>com.ben.mid_term.servlet.ViewLibraryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignupServlet</servlet-name>
        <url-pattern>/SignupServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocationServlet</servlet-name>
        <url-pattern>/LocationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookServlet</servlet-name>
        <url-pattern>/BookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrowServlet</servlet-name>
        <url-pattern>/BorrowServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyBorrowedBookServlet</servlet-name>
        <url-pattern>/MyBorrowedBookServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewMemberServlet</servlet-name>
        <url-pattern>/ViewMemberServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewLibraryServlet</servlet-name>
        <url-pattern>/ViewLibraryServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
